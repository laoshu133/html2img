{
  "name": "html2img",
  "description": "html2img",
  "version": "0.3.0",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git@git.huanleguang.com:static/html2img.git"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "test": "node tests/makeshot.js",
    "test_list": "node tests/makelist.js",
    "dev": "nodemon --ignore status/ --ignore tests/ app.js",
    "start": "pm2 startOrReload package.json"
  },
  "apps": [
    {
      "name": "shot",
      "script": "app.js",
      "exec_mode": "cluster",
      "max_memory_restart": "680M",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "merge_logs": true,
      "instances": 0,
      "env": {
        "NODE_ENV": "production"
      }
    }
  ],
  "dependencies": {
    "bluebird": "^3.4.7",
    "bytes": "^2.4.0",
    "debug": "^2.6.1",
    "dotenv-safe": "^2.3.3",
    "exec-buffer": "^2.0.1",
    "fs-extra-promise": "^0.3.1",
    "hlg-logger": "^0.0.6",
    "jpegtran-bin": "^3.0.6",
    "jquery": "^2.2.4",
    "koa": "^1.2.5",
    "koa-bodyparser": "^2.3.0",
    "koa-favicon": "^1.2.1",
    "koa-onerror": "^1.3.1",
    "koa-router": "^5.4.0",
    "koa-send": "^3.3.0",
    "lodash": "^4.17.4",
    "phantom": "^2.1.21",
    "pidusage": "^1.1.1",
    "pngquant-bin": "^3.1.1"
  },
  "peerDependencies": {
    "pm2": "^2.4.0"
  },
  "devDependencies": {
    "request-promise": "^3.0.0"
  }
}
